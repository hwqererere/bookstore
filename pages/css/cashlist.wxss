
.cashlist{position:fixed;left:0rpx;top:0rpx;width:100%;height:100%;background-color:#f1f0f5;overflow:auto;z-index:0;}
.cashlist .bli{float:left;width:100%;background:#fff;height:100rpx;padding:20rpx 0rpx;margin-bottom:20rpx;}
.cashlist .bli .blileft{float:left;margin-left:30rpx;height:100rpx;}
.cashlist .ordernum{font-size:28rpx;color:#2b2b2b;line-height:40rpx;height:40rpx;}
.cashlist .orderrmb{font-size:28rpx;color:#9d9d9d;line-height:40rpx;height:40rpx;}
.cashlist .ordercreatetime{font-size:20rpx;color:#9d9d9d;line-height: 20rpx;height:20rpx;}
.cashlist .statebut{float:right;width:144rpx;height:56rpx;border-radius:28rpx;font-size:24rpx;line-height: 56rpx;text-align: center;margin-top:20rpx;margin-right:30rpx;}
.cashlist .statebut0{border:2rpx solid #a50000;color:#a50000;}
.cashlist .statebut1{border:2rpx solid #0d9716;color:#0d9716;}